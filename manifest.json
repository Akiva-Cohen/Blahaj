{
  "manifest_version" : 3,
  "name": "Blahaj",
  "description": "make sure your browser always shows the right word for the sea creature",
  "version": "1.1.0",
  "action": {
    "default_icon" : "Icon.png"
  },
  "content_scripts" :[
    {
      "matches" : ["*://*/*"],
      "all_frames" : true,
      "js": ["Actions.js"],
      "run_at" : "document_end"
    }
  ],
  "permissions": ["activeTab", "scripting"]
}